@model IEnumerable<RecipesBook.Models.User>

@{
    ViewBag.Title = "Index";
}

@if (Session["AdminName"] != null)
{
    <text>
        <h2>
            welcome @Session["AdminName"].ToString()
        </h2>
    </text>
}


<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrithDate)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrithDate)
        </td>
      
        <td>
            @Html.ActionLink("Details", "AdminDetails", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>

<br />
